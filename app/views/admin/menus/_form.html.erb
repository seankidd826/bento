<%= form_for(@admin_menu) do |f| %>
  <% if @admin_menu.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@admin_menu.errors.count, "error") %> prohibited this admin_menu from being saved:</h2>

      <ul>
      <% @admin_menu.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :contents %><br>
    <%= f.text_area :contents %>
  </div>
  <div class="field">
    <%= f.label :price %><br>
    <%= f.text_field :price %>
  </div>
  <div class="field">
    <%= f.label :likes_count %><br>
    <%= f.text_field :likes_count %>
  </div>
  <div class="field">
    <%= f.label :in_stock_qty %><br>
    <%= f.text_field :in_stock_qty %>
  </div>
  <div class="field">
    <%= f.label :mominfo %><br>
    <%= f.text_field :mominfo %>
  </div>
  <!-- Check mompic admin -->
  <div class="field">
    <%= f.label :mompic, "Upload pic" %><br>
    <%= f.file_field :mompic %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
