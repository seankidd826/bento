<p id="notice"><%= notice %></p>

<h1>Listing Admin Stories</h1>

<table>
  <thead>
    <tr>
      <th>Momheader</th>
      <th>Momcontents</th>
      <th>Momcontents 1</th>
      <th>Momcontents 2</th>
      <th>Momcontents 3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_stories.each do |admin_story| %>
      <tr>
        <td><%= admin_story.momheader %></td>
        <td><%= admin_story.momcontents %></td>
        <td><%= admin_story.momcontents_1 %></td>
        <td><%= admin_story.momcontents_2 %></td>
        <td><%= admin_story.momcontents_3 %></td>
        <td><%= link_to 'Show', admin_story %></td>
        <td><%= link_to 'Edit', edit_admin_story_path(admin_story) %></td>
        <td><%= link_to 'Destroy', admin_story, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Story', new_admin_story_path %>
